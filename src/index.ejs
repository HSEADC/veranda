<% const {menubar} = require ('./menubar.js') %>

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- Primary Meta Tags -->
    <title>Медиа о современном загороде и дачной жизни</title>
    <meta name="title" content="Медиа о современном загороде и дачной жизни" />
