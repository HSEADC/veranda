      name="description"
      content="Атмосферный и теплый контент на verānda"
    />

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website" />
    <meta
      property="og:url"
      content="https://hseadc.github.io/veranda/index.html"
    />
    <meta
      property="og:title"
      content="Медиа о современном загороде и дачной жизни"
    />
    <meta
      property="og:description"
      content="Атмосферный и теплый контент на verānda"
    />
    <meta property="og:image" content="share/metaCoverIndex.png" />

    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image" />
    <meta
      property="twitter:url"
      content="https://hseadc.github.io/veranda/index.html"
    />
    <meta
      property="twitter:title"
      content="Медиа о современном загороде и дачной жизни"
    />
    <meta
      property="twitter:description"
      content="Атмосферный и теплый контент на verānda"
    />
    <meta property="twitter:image" content="share/metaCoverIndexTwitter.png" />

    <!-- Meta Tags Generated with https://metatags.io -->

    <link rel="shortcut icon" href="<%=require('./images/favicon.svg')%>" />
    <analytics></analytics>
    <title>verānda</title>
  </head>
  <body>
    <!------------------------- Header----------------------->
    <div class="S_MenuBar"><%= menubar %></div>

    <!-- HomeScreen -->
    <section class="S_FirstScreen">
      <div class="O_WelcomeScreen">
        <div class="M_IconHeading">
          <h1 class="A_TitleManrope">ver<em class="Accent">ā</em>nda —</h1>
          <h1 class="A_TitleManrope">ваше</h1>
          <div class="Q_IconMedium Glitter"></div>
          <h1 class="A_TitleManrope">комфортное</h1>
          <h1 class="A_TitleManrope">приключение</h1>
          <h1 class="A_TitleAccent">за городом</h1>
        </div>
      </div>
    </section>

    <!------------------------- HomeArticles----------------------->
    <section class="S_ArticlesHeading">
      <div class="W_HeadingArticles">
        <div class="W_TitleLink">
          <h1 class="A_TitleManrope">
            <em class="Accent">Атмосферные</em> <br />
            и уютные статьи
          </h1>
          <div class="A_LinkArticleNavigation">
            <a href="articles.html" class="A_LinkCategory">Перейти в раздел</a>
            <div class="Q_ArrowForward"></div>
          </div>
        </div>
        <div class="C_ArticlesRow">
          <a class="O_CardArticle" href="articles/decoration.html">
            <div class="Q_Image">
              <img src="<%=require('./images/remonting.png')%>" />
            </div>
            <div class="M_ArticleInfo">
              <div class="M_TagLine">
                <div class="A_Tag">эстетика</div>
                <div class="A_Tag">участок</div>
              </div>
              <h2 class="A_SubCard">
                Рассказываем, как сделать дачу уютной и&nbsp;красивой
                <em>без дорого ремонта</em>
              </h2>
            </div>
          </a>
          <a class="O_CardArticle" href="articles/cherryJam.html">
            <div class="Q_Image">
              <img src="<%=require('./images/Q_CherryJamArticle.png')%>" />
            </div>
            <div class="M_ArticleInfo">
              <div class="M_TagLine">
                <div class="A_Tag">впечатление</div>
                <div class="A_Tag">рецепты</div>
              </div>
              <h2 class="A_SubCard">
                <em>Как в&nbsp;детстве!</em> Делимся рецептом вкуснейшего
                вишневого варенья
              </h2>
            </div>
          </a>
          <a class="O_CardArticle" href="articles/flowersForYou.html">
            <div class="Q_Image">
              <img src="<%=require('./images/Q_FlowersForYou.png')%>" />
            </div>
            <div class="M_ArticleInfo">
              <div class="M_TagLine">
                <div class="A_Tag">сезоны</div>
                <div class="A_Tag">участок</div>
                <div class="A_Tag">эстетика</div>
              </div>
              <h2 class="A_SubCard">
                Собрали подборку <em>неприхотливых, красивых цветов</em> для
                вашей дачи
              </h2>
            </div>
          </a>
        </div>
      </div>
    </section>
    <!------------------------- NewArticles----------------------->
    <section class="S_NewArticle">
      <div class="O_NewArticle">
        <div class="W_NewArticleHeading">
          <div class="A_Tag">Новенькое</div>
          <div class="M_IconHeadingLeft">
            <h1 class="A_TitleManrope">Земля и воля:</h1>
            <h1 class="A_TitleManrope">дача</h1>
            <div class="Q_IconMedium Q_HomeMedium"></div>
            <h1 class="A_TitleManrope">как</h1>
            <h1 class="A_TitleAccent">
              <span class="A_TitleSuperAccent">культурный код</span>
            </h1>
          </div>
          <p class="A_TextMain">
            Дача&nbsp;&mdash; это отличный способ отдохнуть от&nbsp;городской
            суеты и&nbsp;насладиться природой. Проводите время на&nbsp;свежем
            воздухе, занимайтесь садоводством или просто наслаждайтесь тишиной
            и&nbsp;покоем с&nbsp;нами
          </p>
        </div>
        <a href="articles/cultureCode.html" class="A_ButtonPrimary"
          >Читать статью</a
        >
      </div>
      <div class="Q_ImageArticleNew">
        <img src="<%=require('./images/NewArticle.png')%>" alt="" />

        <div class="Q_IconDecoration">
          <img
            src="<%=require('./images/Q_IconDecorationCloud.svg')%>"
            alt=""
          />
        </div>
        <div class="Q_IconDecoration">
          <img src="<%=require('./images/Q_IconDecorationSun.svg')%>" alt="" />
        </div>
      </div>
    </section>
    <!-- ----------------------- IdeasArticles--------------------- -->
    <section class="S_AdvicesArticles">
      <div class="W_AdvicesArticles">
        <div class="W_ArticleHeadingBig">
          <div class="W_TitleLink">
            <div class="M_IconHeadingLeft">
              <h1 class="A_TitleManrope">Свежие идеи</h1>
              <h1 class="A_TitleAccent">для отдыха</h1>
              <div class="Q_IconMedium Snowflakes"></div>
            </div>
            <div class="A_LinkArticleNavigation">
              <a href="ideas.html" class="A_LinkCategory">Перейти в раздел</a>
              <div class="Q_ArrowForward"></div>
            </div>
          </div>
          <a href="ideas/kite.html" class="O_ArticleWide rainingBg">
            <div class="M_ArticleInfo">
              <div class="M_TagLine">
                <div class="A_Tag">релакс</div>
                <div class="A_Tag">впечатлиния</div>
              </div>
              <h2 class="A_SubCard">
                <em>Погода не&nbsp;страшна! </em>
                Прыгаем по&nbsp;лужам и&nbsp;занимаемся полезностями
              </h2>
            </div>
          </a>
        </div>
        <div class="C_SmallArticleCards">
          <a class="M_AdviceCard" href="ideas/drinks.html">
            <h2 class="A_SubCard">
              Напитки для <em>тихого вечера</em>
              на&nbsp;крыльце
            </h2>
            <div class="M_TagLine">
              <div class="A_Tag">рецепты</div>
            </div>
          </a>
          <a class="M_AdviceCard" href="ideas/herbs.html">
            <h2 class="A_SubCard">
              Ищем <em>полезные травы</em> у&nbsp;себя в&nbsp;огроде
            </h2>
            <div class="M_TagLine">
              <div class="A_Tag">здоровье</div>
              <div class="A_Tag">участок</div>
            </div>
          </a>
          <a class="M_AdviceCard" href="ideas/collectingFlowers.html">
            <h2 class="A_SubCard">
              Собираем <em>полевые цветы</em> и&nbsp;плетем пышные венки
            </h2>
            <div class="M_TagLine">
              <div class="A_Tag">эстетика</div>
              <div class="A_Tag">впечатление</div>
            </div>
          </a>
        </div>
      </div>
    </section>
    <!------------------------- QaArticles----------------------->
    <section class="S_QaCards">
      <div class="W_QaCards">
        <div class="W_TitleLink">
          <div class="M_IconHeadingLeft">
            <h1 class="A_TitleManrope">Отвечаем на</h1>
            <h1 class="A_TitleAccent">
              <span class="A_TitleSuperAccent">дачные</span>
            </h1>
            <div class="Q_IconMedium Clouds"></div>
            <h1 class="A_TitleManrope">вопросы</h1>
          </div>
          <div class="A_LinkArticleNavigation">
            <a href="queAnswer.html" class="A_LinkCategory">Перейти в раздел</a>
            <div class="Q_ArrowForward"></div>
          </div>
        </div>
        <div class="C_QaCards">
          <a href="QA/notFreeze.html" class="O_QaCard">
            <div class="M_ArticleInfo">
              <div class="M_TagLine">
                <div class="A_Tag">здоровье</div>
                <div class="A_Tag">сезоны</div>
              </div>
              <h2 class="A_SubCard">
                Как не&nbsp;замерзнуть на&nbsp;даче в&nbsp;холода
              </h2>
            </div>
            <div class="Q_QaIconArticle">
              <img src="<%=require('./images/Q_QAIconSnow.png')%>" />
            </div>
          </a>
          <a href="QA/queAns.html" class="O_QaCard">
            <div class="M_ArticleInfo">
              <div class="M_TagLine">
                <div class="A_Tag">эстетика</div>
              </div>
              <h2 class="A_SubCard">Лучшее время для съемки заката</h2>
            </div>
            <div class="Q_QaIconArticle">
              <img src="<%=require('./images/Q_QAIconSun.png')%>" alt="" />
            </div>
          </a>
          <a href="QA/komari.html" class="O_QaCard">
            <div class="M_ArticleInfo">
              <div class="M_TagLine">
                <div class="A_Tag">здоровье</div>
                <div class="A_Tag">сезоны</div>
              </div>
              <h2 class="A_SubCard">Что лучше всего отпугивает комаров</h2>
            </div>
            <div class="Q_QaIconArticle">
              <img src="<%=require('./images/Q_QAIconMosquito.png')%>" />
            </div>
          </a>
          <a href="QA/taxes.html" class="O_QaCard">
            <div class="M_ArticleInfo">
              <div class="M_TagLine">
                <div class="A_Tag">участок</div>
              </div>
              <h2 class="A_SubCard">Как оплачивать налог за&nbsp;дачу</h2>
            </div>
            <div class="Q_QaIconArticle">
              <img src="<%=require('./images/Q_QAIconHome.png')%>" />
            </div>
          </a>
        </div>
      </div>
    </section>
    <!------------------------- Footer----------------------->
    <footer class="S_Footer">
      <div class="W_FooterWithout">
        <div class="O_FooterCategoriesForm">
          <div class="W_FooterCategories">
            <a href="index.html" class="A_FooterCategoryList">Главная</a>
            <a href="about.html" class="A_FooterCategoryList">О медиа</a>

            <a href="articles.html" class="A_FooterCategoryList">Статьи</a>
            <a href="ideas.html" class="A_FooterCategoryList"
              >Идеи для отдыха</a
            >
            <a href="queAnswer.html" class="A_FooterCategoryList"
              >Вопрос-ответ</a
            >
          </div>
          <div class="A_SubscriptionForm">
            <div class="M_FooterLogoSocial">
              <div class="A_NavigationLogo A_NavigationLogoCream"></div>
              <div class="C_Socials">
                <a class="A_Telegram" href="https://t.me/veranda_media"></a>
                <a class="A_Vk" href="https://vk.com/public223193055"></a>
                <a
                  class="A_YouTube"
                  href="https://www.youtube.com/@veranda2023"
                ></a>
              </div>
            </div>
          </div>
        </div>
        <div class="W_FooterNamesSocialLogo">
          <div class="W_Authors">
            <div class="W_FooterNamesList">
              <p class="AuthorName">Кураторы</p>
              <p class="AuthorName">Софья Каем</p>
              <p class="AuthorName">Анна Комкова</p>
            </div>
            <div class="W_FooterNamesList">
              <p class="AuthorName">Дизайнеры</p>
              <p class="AuthorName">Ева Киприанова</p>
              <p class="AuthorName">Злата Баева</p>
              <p class="AuthorName">Максим Кириллов</p>
            </div>
            <div class="W_FooterNamesList">
              <p class="AuthorName">HSE ADC</p>
              <p class="AuthorName">2023-2024</p>
            </div>
          </div>
        </div>
      </div>
    </footer>
